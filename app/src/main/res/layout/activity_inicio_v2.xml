<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="econtact.org.myapp.Inicio_v2"
    android:background="@drawable/fondo640x960_lan">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:baselineAligned="false"
        android:showDividers="middle"
        android:gravity="center_vertical"
        android:weightSum="1"
        android:id="@+id/linearLayout6"
        android:layout_gravity="center"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="72dp">

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnIVR"
            android:background="@drawable/lanpass"
            android:layout_alignTop="@+id/btnContactCenter"
            android:layout_toLeftOf="@+id/linearLayout6"
            android:layout_toStartOf="@+id/linearLayout6" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnContactCenter"
            android:background="@drawable/contacto"
            android:layout_above="@+id/linearLayout6"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

    </LinearLayout>

</RelativeLayout>
